{"line":143,"column":0},"source":"component/ConductTransaction.js","original":{"line":55,"column":16}},{"generated":{"line":143,"column":19},"source":"component/ConductTransaction.js","original":{"line":35,"column":12}},{"generated":{"line":143,"column":20},"source":"component/ConductTransaction.js","original":{"line":34,"column":8}},{"generated":{"line":144,"column":0},"source":"component/ConductTransaction.js","original":{"line":58,"column":5}},{"name":"Component","generated":{"line":148,"column":2},"source":"component/ConductTransaction.js","original":{"line":6,"column":33}},{"generated":{"line":150,"column":0},"source":"component/ConductTransaction.js","original":{"line":59,"column":1}},{"name":"ConductTransaction","generated":{"line":151,"column":15},"source":"component/ConductTransaction.js","original":{"line":61,"column":15}}],"sources":{"component/ConductTransaction.js":"import React, { Component } from 'react';\r\nimport { FormGroup, FormControl, Button  } from 'react-bootstrap';\r\nimport { Link } from 'react-router-dom';\r\nimport history from '../history';\r\n\r\nclass ConductTransaction extends Component {\r\n    state = { recipient: '', amount: 0 };\r\n\r\n    updateRecipient = event => {\r\n        this.setState({ recipient: event.target.value });\r\n    }\r\n\r\n    updateAmount = event => {\r\n        this.setState({ amount: Number(event.target.value) });\r\n    }\r\n\r\n    conductTransaction = () => {\r\n        const { recipient, amount  } = this.state;\r\n\r\n        fetch(`${document.location.origin}/api/transact`, {\r\n            method: 'POST',\r\n            headers: {'Content-Type': 'application/json'},\r\n            body: JSON.stringify({recipient, amount })\r\n        }).then(response => response.json())\r\n        .then(json => {\r\n            alert(json.message || json.type);\r\n            history.push('/transaction-pool');\r\n        });\r\n    }\r\n\r\n    render() {\r\n\r\n\r\n        return (\r\n            <div className='ConductTransaction'>\r\n\r\n                <Link to='/'>Home</Link>\r\n                <h3>Conduct a Transaction</h3>\r\n                <FormGroup>\r\n                    <FormControl \r\n                    input='text' \r\n                    placeholder='recipient' \r\n                    value={this.state.recipient}\r\n                    onChange={this.updateRecipient}\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <FormControl\r\n                        input='number'\r\n                        placeholder='amount'\r\n                        value={this.state.amount}\r\n                        onChange={this.updateAmount}\r\n                    />\r\n                </FormGroup>\r\n                <Button bsSize='small' bsStyle='danger' onClick={this.conductTransaction}> Submit</Button>\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nexport default ConductTransaction;"},"lineCount":null}},"hash":"1339f0b467de4ab93fce4e95a832c123","cacheData":{"env":{}}}