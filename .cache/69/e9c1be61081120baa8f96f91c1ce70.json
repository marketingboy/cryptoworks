olumn":17},"source":"component/Block.js","original":{"line":49,"column":25}},{"name":"toggletransaction","generated":{"line":131,"column":22},"source":"component/Block.js","original":{"line":49,"column":30}},{"generated":{"line":132,"column":0},"source":"component/Block.js","original":{"line":47,"column":16}},{"generated":{"line":132,"column":21},"source":"component/Block.js","original":{"line":44,"column":12}},{"generated":{"line":132,"column":22},"source":"component/Block.js","original":{"line":43,"column":8}},{"generated":{"line":133,"column":0},"source":"component/Block.js","original":{"line":55,"column":5}},{"name":"Component","generated":{"line":137,"column":2},"source":"component/Block.js","original":{"line":5,"column":20}},{"name":"Block","generated":{"line":139,"column":15},"source":"component/Block.js","original":{"line":75,"column":15}}],"sources":{"component/Block.js":"import React, {Component } from 'react';\r\nimport { Button } from 'react-bootstrap'; \r\nimport Transaction from './Transaction';\r\n\r\nclass Block extends Component {\r\n\r\n    state = { displayTransaction: false };\r\n\r\n    toggletransaction = () => {\r\n        this.setState({ displayTransaction: !this.state.displayTransaction });\r\n\r\n    }\r\n\r\n     get displayTransaction()  {\r\n         const { data } = this.props.block;\r\n         const stringifiedData = JSON.stringify(data);\r\n\r\n         const dataDisplay = stringifiedData.length > 35 ? `${stringifiedData.substring(0, 35)}...` : stringifiedData;\r\n\r\n        if(this.state.displayTransaction){\r\n            return (\r\n                <div>\r\n                    {\r\n                        data.map(transaction => (\r\n                            <div key={transaction.id} > \r\n                                <hr />\r\n                                <Transaction transaction={transaction} />\r\n                            </div>\r\n                        ))\r\n                    }\r\n                    <br /> \r\n\r\n                    <Button bsStyle=\"danger\"\r\n                        bsSize=\"small\"\r\n                        onClick={this.toggletransaction}\r\n                    >\r\n                        Show Less\r\n                </Button>\r\n                </div>\r\n            );\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                <div>Data: {dataDisplay}</div>\r\n                \r\n                <Button bsStyle=\"danger\" \r\n                bsSize=\"small\" \r\n                onClick={this.toggletransaction}\r\n                >\r\n                    Show More\r\n                </Button>\r\n            </div>\r\n        );\r\n    }\r\n\r\n    render(){\r\n        console.log('this.displaytransaction', this.displayTransaction)\r\n        const { timestamp, hash } = this.props.block;\r\n\r\n        const hashDisplay = `${hash.substring(0, 15)}...`;\r\n        \r\n\r\n        return (\r\n            <div className='Block'>\r\n                <div>Hash: {hashDisplay}</div>\r\n                <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\r\n               \r\n                {this.displayTransaction}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Block;"},"lineCount":null}},"hash":"c6b8b550343153673dd2ac3c9c49acde","cacheData":{"env":{}}}